<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:state_pressed="false" >
        <shape
            android:shape="rectangle"  >
            <corners
                android:radius="10dip" />
            <stroke
                android:width="1dip"
                android:color="#8b7d52" />
            <gradient
                android:angle="-90"
                android:startColor="#8c7d52"
                android:endColor="#baad8a"  />
        </shape>
    </item>
    <item
        android:state_focused="false">
        <shape
            android:shape="rectangle"  >
            <corners
                android:radius="10dip" />
            <stroke
                android:width="1dip"
                android:color="#8a7d52" />
            <solid
                android:color="#897d52"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners
                android:radius="10dip" />
            <stroke
                android:width="1dip"
                android:color="#887d52" />
            <gradient
                android:angle="-90"
                android:startColor="#8c7d52"
                android:endColor="#baad8a" />
        </shape>
    </item>
</selector>